<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:edu.usc.epigenome.eccp.client.controlPanel"
	xmlns:f="urn:import:edu.usc.epigenome.eccp.client.pane.flowcellReport">
	
	<ui:with field="res" type="edu.usc.epigenome.eccp.client.Resources.UserPanelResources"></ui:with>
	
 	<g:SplitLayoutPanel>
     	<g:west size="250">
     	<g:ScrollPanel>
     	<g:HTMLPanel ui:field="layoutReport">
     		 <!--  <c:ControlPanelWidget typeGeneus="Samples From Geneus" typeGroup="ShowSamples"/>-->
     		 <g:Label ui:field="label" text="Switch to Sample View" styleName='{res.userPanel.viewSwitchlabel}'></g:Label>
     	    <g:HTMLPanel ui:field="FcellReport">
     		 	<c:ControlPanelWidget typeGeneus="Flowcells From Geneus" typeGroup="ShowGeneus"/>
     		 </g:HTMLPanel>
     	</g:HTMLPanel>	
     	</g:ScrollPanel>
     	</g:west>
     	<g:center>
     		<g:ScrollPanel>
     			<g:VerticalPanel ui:field="addTabPanel"></g:VerticalPanel>
     		</g:ScrollPanel>
     	</g:center>
    </g:SplitLayoutPanel>
 </ui:UiBinder>