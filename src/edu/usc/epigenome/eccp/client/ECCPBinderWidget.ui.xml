<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:edu.usc.epigenome.eccp.client.controlPanel"
	xmlns:f="urn:import:edu.usc.epigenome.eccp.client.pane.flowcellReport">
 	<g:DockLayoutPanel unit='EM'>
     	<g:west size="15">
     		<g:StackLayoutPanel unit="EM" height="500px">
     		 	<g:stack>
     		 		<g:header size='2'>Flowcell Reports</g:header>
     		 		<g:HTMLPanel ui:field="FcellReport">
     		 			<c:ControlPanelWidget typeGeneus="Samples from Geneus" typeGroup="ShowSamples"/>
     		 			<c:ControlPanelWidget typeGeneus="All Flowcells" typeGroup="ShowAll"/>
     		 			<c:ControlPanelWidget typeGeneus="Flowcells from Geneus" typeGroup="ShowGeneus"/>
     		 			<c:ControlPanelWidget typeGeneus="Flowcells from Disk" typeGroup="ShowFS"/>
     		 			<c:ControlPanelWidget typeGeneus="Incomplete Flowcells" typeGroup="ShowIncomplete"/>
     		 		</g:HTMLPanel>
     		 	</g:stack>
     		 	<g:stack>
     		 		<g:header size='2'>Methylation Reports</g:header>
     		 		<g:HTMLPanel ui:field="MethylaReport">
     		 			<c:ControlPanelWidget typeGeneus="Infinium Methylation 27K" typeGroup="MethylationReports"/>
     		 			<c:ControlPanelWidget typeGeneus="Methylation Sanity Check" typeGroup="MethylationSanity"/>
     		 		</g:HTMLPanel>
     		 	</g:stack>
     		</g:StackLayoutPanel>
     	</g:west>
     	<g:center>
     		<g:ScrollPanel>
     			<g:VerticalPanel ui:field="addTabPanel"></g:VerticalPanel>
     		</g:ScrollPanel>
     	</g:center>
    </g:DockLayoutPanel>
 </ui:UiBinder>