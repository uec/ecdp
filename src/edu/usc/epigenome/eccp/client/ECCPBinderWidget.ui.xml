<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
             xmlns:g="urn:import:com.google.gwt.user.client.ui" 
             xmlns:c="urn:import:edu.usc.epigenome.eccp.client.sampleList" 
             xmlns:f="urn:import:edu.usc.epigenome.eccp.client.pane.flowcellReport">

	<ui:with field="res" type="edu.usc.epigenome.eccp.client.Resources.UserPanelResources"></ui:with>

	<g:SplitLayoutPanel>
		<g:west size="600">
			<g:ScrollPanel>
				<g:HTMLPanel ui:field="layoutReport">
					<c:sampleList/>
				</g:HTMLPanel>
			</g:ScrollPanel>
		</g:west>
		<g:center>
			<g:ScrollPanel>
				<g:VerticalPanel ui:field="addTabPanel"></g:VerticalPanel>
			</g:ScrollPanel>
		</g:center>
	</g:SplitLayoutPanel>
</ui:UiBinder>