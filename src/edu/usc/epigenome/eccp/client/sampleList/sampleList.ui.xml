<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
             xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container" 
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client">
	<gxt:ContentPanel ui:field="gridPanel" headingText="USC Epigenome Center">
		<container:VerticalLayoutContainer>
			<container:child>
				<gxt:toolbar.ToolBar>
					<gxt:button.TextButton ui:field="byFlowcell" text="By Flowcell" />
					<gxt:button.TextButton ui:field="byLibrary" text="By Library" />
					<gxt:button.TextButton ui:field="byProject" text="By Project" />
					<gxt:button.TextButton ui:field="expand" text="Expand All" />
					<gxt:button.TextButton ui:field="collapse" text="Collapse All" />
				</gxt:toolbar.ToolBar>
			</container:child>
			<container:child>
				<gxt:container.VerticalLayoutContainer ui:field="content" />
			</container:child>
		</container:VerticalLayoutContainer>
	</gxt:ContentPanel>
</ui:UiBinder> 