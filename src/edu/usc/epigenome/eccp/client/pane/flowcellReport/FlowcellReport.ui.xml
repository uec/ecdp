<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with field="res" type="edu.usc.epigenome.eccp.client.Resources.UserPanelResources"></ui:with>
	
	<g:FlowPanel ui:field="mainPanel">
		<g:FlowPanel ui:field="searchPanel" styleName='{res.userPanel.flowcellsearch}'>
			<g:HorizontalPanel ui:field="searchOptionsPanel">
				<g:Label text="Flowcell Properties: "/>
				<g:TextBox ui:field="globalSearchBox"/>
				<g:Label text="Lane Properties: "/>
				<g:TextBox ui:field="laneSearchBox"/>
				<g:Button ui:field="searchButton" text="Search"/>
			</g:HorizontalPanel>
		</g:FlowPanel> 
		<g:FlowPanel ui:field="vp"></g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 