<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="res" type="edu.usc.epigenome.eccp.client.Resources.UserPanelResources"></ui:with>
	
	<g:FlowPanel ui:field='LabPanel'>
		<g:HorizontalPanel>
			<g:Label ui:field="view" text="viewChart" styleName='{res.userPanel.viewdisplaylabel}'></g:Label>
		</g:HorizontalPanel>
		<g:FlowPanel ui:field="popup" styleName='{res.userPanel.popupdisplaypanel}'>
			<g:FlowPanel ui:field='mainPanel' styleName='{res.userPanel.popupdisplaypanel}'>
				<g:FlowPanel ui:field='chartPanel'></g:FlowPanel>
				<g:Button ui:field="closeButton" text="Close"></g:Button>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 