<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with field="res" type="edu.usc.epigenome.eccp.client.Resources.UserPanelResources"></ui:with>
	
	<g:FlowPanel ui:field="vp">
		<g:FlowPanel ui:field="vPanel">
			<g:MenuBar ui:field='menu'>
				<g:MenuItem ui:field='Refresh'>Refresh</g:MenuItem>
			</g:MenuBar>
			<g:HorizontalPanel ui:field="jobCounts">
				<g:Label ui:field="queuedCountLabel" text=" 0 jobs queued -- "></g:Label>
				<g:Label ui:field="runningCountLabel" text=" 0 jobs running -- "></g:Label>
				<g:Label ui:field="heldCountLabel" text=" 0 jobs held "></g:Label>
			</g:HorizontalPanel>
			<g:FlexTable ui:field="pbsTable" styleName='{res.userPanel.LaneTable}'></g:FlexTable>
		</g:FlowPanel>	
	</g:FlowPanel>
</ui:UiBinder> 