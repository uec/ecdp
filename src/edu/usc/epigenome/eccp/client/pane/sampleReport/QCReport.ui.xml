<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel ui:field='LabPanel'>
		<g:Label ui:field="Statistics" text="Summary Statistics"></g:Label>
		<g:DecoratedPopupPanel ui:field='popup'>
			<g:FlowPanel ui:field='mainPanel'>
				<g:FlowPanel ui:field='summaryChart'></g:FlowPanel>
				<g:Button ui:field='closeButton' text="Close"></g:Button>
			</g:FlowPanel>
		</g:DecoratedPopupPanel>
	</g:FlowPanel>
</ui:UiBinder> 