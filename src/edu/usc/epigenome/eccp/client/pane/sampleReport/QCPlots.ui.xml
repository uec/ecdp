<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="res" type="edu.usc.epigenome.eccp.client.Resources.UserPanelResources"></ui:with>
	
	<g:FlowPanel ui:field='LabPanel'>
		<g:Label ui:field="showPlots" text="QC Plots" styleName='{res.userPanel.viewdisplaylabel}'></g:Label>
		<g:FlowPanel ui:field="popup" styleName='{res.userPanel.popupdisplaypanel}'>
			<g:FlowPanel ui:field='mainPanel' styleName='{res.userPanel.popupdisplaypanel}'>
				<g:FlowPanel ui:field='summaryChart'>
					<g:Label ui:field='path'></g:Label>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 